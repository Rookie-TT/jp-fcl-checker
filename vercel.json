{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "api/index.py"
    },
    {
      "src": "/check",
      "dest": "api/index.py"
    },
    {
      "src": "/batch",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)\\.(js|css|png|jpg|svg|ico)",
      "status": 404
    }
  ]
}
